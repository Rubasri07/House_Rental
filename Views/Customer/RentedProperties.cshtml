@model IEnumerable<House_Rental_System.Models.Sold_Property>

@{
    ViewBag.Title = "RentedProperties";
    Layout = "~/Views/Shared/_CustomerLayout.cshtml";
}




<div class="table-responsive">
    <table class="table table-success table-striped" style="margin-top:1%;">
        <tr>
            <th>
                Type
            </th>
            <th>
                Seller Name
            </th>
            <th>
                Rent
            </th>
            <th>
                Deposit
            </th>
            <th>
                Date of Sale
            </th>
            
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Property_Details.Property_Type)
                    
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Seller_Details.Seller_Name)
                    
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Rental_Amount)
                    
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Deposit_Amount)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Date_of_Sale)
                </td>
            </tr>
        }

    </table>
</div>